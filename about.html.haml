            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          / Collect the nav links, forms, and other content for toggling
        %a.navbar-brand(href="index.html")
          %img.img-responsive.logo(src="img/logo.svg")
        %div.collapse.navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li.pressed
              %a(href="projects.html")
                PROJECTS
            %li
              %a(href="http://helloandi.tumblr.com/") 
                BLOG
            %li
              %a(href="about.html") 
                ABOUT
        
      // %img.img-responsive.section-pic.andrea-cartoon(src="img/andrea.jpg")
      %div.container.work
        %div.row
          %div.col-xs-24.col-md-12
            %img.img-responsive.section-pic(src="img/andrea2.jpg")
            %div.section-title
              ABOUT ME
